name: ga-tutorial
on:
  workflow_dispatch:
    inputs:
      release-version:
        description: 'Release version tag'
        default: 'v1.0.0'
        required: true
jobs:
  default:
    runs-on: ubuntu-latest
    steps:
        - uses: actions/checkout@v2
          uses: actions/setup-node@v2
          with:
            node-version: '16'
          run: echo "console.log('******', process.env.PENDO_APP_ID)" > ./client.js
          run: node client.js
          env:
            PENDO_APP_ID: 'blablabla!'
